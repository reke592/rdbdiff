{
  "name": "@reke592/rdbdiff",
  "version": "0.0.0",
  "description": "Relational DB schema diff",
  "bin": {
    "rdbdiff": "./bin/rdbdiff.js"
  },
  "keywords": [
    "database",
    "diff",
    "cli",
    "npmtool"
  ],
  "main": "./lib/cli.js",
  "type": "commonjs",
  "scripts": {
    "test": "node . compare mysql://root:dev@localhost:3306/A mysql://root:dev@localhost:3306/B",
    "dev": "ts-node ./src/cli.ts compare mysql://root:dev@localhost:3306/A mysql://root:dev@localhost:3306/B",
    "dev:eager": "ts-node ./src/cli.ts compare -e mysql://root:dev@localhost:3306/A mysql://root:dev@localhost:3306/B",
    "clean": "rimraf ./lib",
    "build": "npm run clean && tsc",
    "prepublishOnly": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "commander": "^13.0.0",
    "knex": "^3.1.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/node": "^22.10.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
  }
}
